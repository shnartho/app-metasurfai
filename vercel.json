{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "static": {
      "public": "public"
    },
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/static/(.*)",
        "destination": "/public/Static/$1"
      }
    ]
  }
  